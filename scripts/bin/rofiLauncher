#!/bin/bash

WORKINGDIR="$HOME/bin"
MAP="$WORKINGDIR/launcher.csv"

cat "$MAP" \
    | cut -s -d ',' -f 1 \
    | rofi -dmenu -i -p "Util " \
    | head -n 1 \
    | xargs -i --no-run-if-empty grep "{}" "$MAP" \
    | cut -d ',' -f 2 \
    | sed -e 's/^\s*//'\
    | head -n 1 \
    | xargs -i --no-run-if-empty /bin/bash -c "{}"

exit 0
