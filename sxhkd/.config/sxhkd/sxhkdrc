# super + {_,shift +,ctrl +,shift + ctrl +} key 
#     {_,_,_,_}

super + ctrl + b
    terminal -e bmenu

super + c
    exec blueman-manager

super + d
    rofi -modi "drun#ssh" -show drun

super + shift + d
    killall dunst; exec notify-send 'restart dunst'

super + {_,shift +, ctrl +} i
    exec {~/bin/rofiLauncher,htstr,addhtstr}

super + n
    keepass $KEEFILE

super + t
    pkill compton

super + ctrl + t
    compton -b

super + shift + u
    caffeine kill; exec caffeine

super + u
    rofimoji -s light --use-clipboard 

super + ctrl + @x
    xkill

super + F2
    brave

super + {_,shift +,ctrl +,shift + ctrl +} F3
    {terminal -e lf,pcmanfm,_,_}

super + F4
    mailspring

#super + F5

Print
    flameshot gui

super + @Print
    i3-scrot -w

super + shift + @Print
    i3-scrot -s

super + shift + semicolon
    xdg-open /usr/share/doc/manjaro/i3_help.pdf

XF86AudioPlay
    playerctl -p spotify play-pause
    
